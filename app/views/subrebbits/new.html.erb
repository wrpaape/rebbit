<%= form_tag(@subrebbit, class: "form-group" ) do %>
  <div class="form-inline">
    <div class="col-sm-12 form-group center"></br>
      <label for="category" class="white control-label">new subrebbit</label>
      <%= text_field_tag(:category, nil, { placeholder: "dead horse", id: "category", class: "black" })  %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12 black center"></br>
      <%= submit_tag "submit", class: "black btn btn-default" %>
    </div>
  </div>
<% end %>
